{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:cov:html": "vitest run --coverage --reporter=html",
    "test:watch": "vitest",
    "test:unit": "vitest run src/test/unit",
    "test:integration": "vitest run src/__tests__ --exclude **/*.{smoke,a11y,perf}.test.*",
    "test:smoke": "vitest run **/*.smoke.test.*",
    "test:a11y": "vitest run **/*.a11y.test.*",
    "test:perf": "vitest run **/*.perf.test.*",
    "test:changed": "vitest related",
    "test:profile": "vitest run --reporter=verbose",
    "analyze": "cross-env ANALYZE=1 vite build && echo Report: dist/bundle-report.html"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest-axe": "^9.0.0",
    "jsdom": "^24.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite": "^7.1.2",
    "vitest": "^2.1.4"
    ,"@vitest/coverage-v8": "^2.1.4"
  }
}
